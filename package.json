{
  "name": "founder-funnel",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "[ \"$NODE_ENV\" = \"production\" ] && echo 'Skipping prepare in production' || nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ship": "git push",
    "docker": "docker build --memory=4g -f deploy/Dockerfile -t funnel . && docker run --env-file .env --rm -p 3000:3000 funnel",
    "repomix": "repomix --ignore='public,**/public/**,context,**/assets/**,**/decisions/**,.agent,'",
    "repomix:new": "{ git ls-files --others --exclude-standard; git diff --name-only; git diff --cached --name-only; } | sort -u | repomix --stdin -o repomix-changed.xml"
  },
  "dependencies": {
    "@iconify/json": "^2.2.421",
    "@internationalized/date": "^3.10.1",
    "@nuxt/content": "^3.10.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.2",
    "@nuxt/test-utils": "3.20.1",
    "@nuxt/ui": "4.3.0",
    "@nuxtjs/seo": "3.2.2",
    "@unhead/vue": "^2.1.1",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "14.0.0",
    "better-sqlite3": "^12.5.0",
    "canvas-confetti": "^1.9.4",
    "nodemailer": "^7.0.12",
    "nuxt": "^4.2.2",
    "nuxt-llms": "^0.1.3",
    "nuxt-studio": "1.0.0",
    "typescript": "^5.9.3",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@compodium/nuxt": "0.1.0-beta.12",
    "@nuxt/eslint": "1.12.1",
    "@nuxt/hints": "1.0.0-alpha.5",
    "@types/node": "^22.19.3",
    "@types/nodemailer": "^7.0.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  },
  "packageManager": "pnpm@10.12.0"
}
