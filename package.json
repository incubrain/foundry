{
  "name": "founder-funnel",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run --filter @incubrain/founder-funnel dev",
    "build": "pnpm run --filter @incubrain/founder-funnel build",
    "prepare": "pnpm -r prepare > prepare.log 2>&1",
    "dev:web": "nuxt dev examples/starter/web",
    "dev:docs": "nuxt dev examples/starter/docs",
    "dev:ff": "nuxt dev examples/founder-funnel/web",
    "dev:ff:docs": "nuxt dev examples/founder-funnel/docs",
    "dev:ae": "nuxt dev examples/astronera",
    "build:ae": "nuxt build examples/astronera > build.log 2>&1",
    "generate": "pnpm -r generate",
    "preview": "pnpm --filter web preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ship": "git push",
    "skills:update": ".claude/skills/update-skills.sh",
    "docker": "docker build --memory=4g -f deploy/Dockerfile.web -t funnel . && docker run --env-file .env --rm -p 3000:3000 funnel",
    "repomix": "repomix --ignore='public,**/public/**,context,**/assets/**,**/decisions/**,.claude,**/content/**/*.{md,yml},apps/docs/**.md'",
    "repomix:new": "{ git ls-files --others --exclude-standard; git diff --name-only; git diff --cached --name-only; } | sort -u | repomix --stdin -o repomix-changed.xml",
    "typecheck": "nuxt typecheck layer",
    "verify": "pnpm run dev:prepare && pnpm run lint && pnpm run typecheck",
    "layer:release": "release-it --config .release-it.layer.json",
    "layer:publish": "cd layer && npm publish --access public",
    "release": "pnpm run layer:release && pnpm run layer:publish",
    "cli:build": "pnpm --filter create-founder-funnel build",
    "cli:publish": "cd cli && npm publish --access public",
    "kanban": "npx vibe-kanban"
  },
  "dependencies": {
    "@iconify/json": "^2.2.421",
    "@internationalized/date": "^3.10.1",
    "@nuxt/content": "^3.10.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.0",
    "@nuxt/test-utils": "3.20.1",
    "@nuxt/ui": "4.3.0",
    "@nuxtjs/seo": "3.2.2",
    "@unhead/vue": "^2.1.1",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "14.0.0",
    "better-sqlite3": "^12.5.0",
    "canvas-confetti": "^1.9.4",
    "nodemailer": "^7.0.12",
    "nuxt": "^4.3.0",
    "nuxt-llms": "^0.1.3",
    "nuxt-studio": "1.0.0",
    "typescript": "^5.9.3",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@compodium/nuxt": "0.1.0-beta.12",
    "@nuxt/eslint": "1.12.1",
    "@nuxt/hints": "1.0.0-alpha.5",
    "@types/node": "^22.19.3",
    "@types/nodemailer": "^7.0.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  },
  "packageManager": "pnpm@10.12.0"
}
